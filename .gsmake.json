{
    "name":"github.com/gschat/tsdb",

    "domain":"golang|gslang",

    "import":[
        {"name" : "github.com/gsmake/golang","domain":"task","version":"release/v3.0"},
        {"name" : "github.com/gsrpc/gsrpc","domain":"task|gslang","version":"release/v1.0"},
        {"name" : "github.com/gsrpc/gorpc","domain":"golang","version":"release/v1.0"},
        {"name" : "github.com/gsdocker/gsos","domain":"golang","version":"v2.0"},
        {"name" : "github.com/gsdocker/gserrors","domain":"golang","version":"v2.0"},
        {"name" : "github.com/gsdocker/gsconfig","domain":"golang","version":"v2.0"},
        {"name" : "github.com/gsdocker/gslogger","domain":"golang","version":"v2.0"},
        {"name" : "github.com/syndtr/goleveldb","domain":"golang","version":"master"},
        {"name" : "github.com/golang/snappy","domain":"golang","version":"master"},
        {"name" : "github.com/ssdb/gossdb","domain":"golang","version":"master"}
    ],

    "properties":{

        "gsmake.golang.binary" :{
              "leveldb-bench" : "leveldb",
              "ssdb-bench" : "ssdb"
        },

        "gsrpc":{
            "golang":["./tsdb.gs"]
        },

        "gsmake.golang.test" :[
           {"dir":"./test","flags":"-bench . -cpu 4 "}
       ]
    }
}
